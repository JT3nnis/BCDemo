@{
    ViewBag.AppName = "BuildersCapital";
    ViewBag.Title = "Documents";
}

<div>
    @Html.AntiForgeryToken()
    @Html.Partial("_FileUploadParstial")
    <div id="appGrid" class="app-grid">
        <div id="appDataGrid" class="data-grid"></div>
    </div>
</div>

@section scripts
{
    @Scripts.Render("~/Scripts/app/appfile.js")
    @Scripts.Render("~/Scripts/app/builderscapital.js")

    <script type="text/javascript">
        $(document).ready(function () {
            AppWeb.File.initUpload();
            AppWeb.BuildersCapital.init();
        });
    </script>

}